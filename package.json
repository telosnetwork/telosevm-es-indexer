{
  "name": "telosevm-translator",
  "version": "0.1a5",
  "description": "State History EVM Translator/Indexer",
  "author": "guilledk",
  "license": "MIT",
  "repository": "https://github.com/telosnetwork/telosevm-translator",
  "scripts": {
    "bootstrap": "sh -c 'yarn && cd node_modules/@eosrio/hyperion-sequential-reader && npm i && npx tsc'",
    "build": "npm run bootstrap && npx tsc",
    "start": "node --expose-gc build/main.js",
    "test": "mocha build/tests/**/*.spec.js --timeout 120000",
    "elastic-start": "node scripts/elastic-start.mjs",
    "elastic-stop": "node scripts/elastic-stop.mjs",
    "kibana-start": "node scripts/kibana-start.mjs",
    "kibana-stop": "node scripts/kibana-stop.mjs",
    "docker-build": "npm run build && node scripts/docker-build.mjs",
    "docker-start": "npm run docker-build && node scripts/docker-run.mjs",
    "docker-stop": "node scripts/docker-stop.mjs",
    "clean-deps": "rm -rdf node_modules && yarn cache clean"
  },
  "type": "module",
  "dependencies": {
    "@elastic/elasticsearch": "8.10.0",
    "@eosrio/hyperion-sequential-reader": "https://github.com/eosrio/hyperion-sequential-reader.git#hist_buf_size",
    "@eosrio/node-abieos": "2.1.1",
    "@ethereumjs/block": "3.6.3",
    "@ethereumjs/common": "2.6.5",
    "@ethereumjs/rlp": "4.0.1",
    "@ethereumjs/trie": "5.0.5",
    "@ethereumjs/tx": "3.5.2",
    "@ethereumjs/util": "8.0.6",
    "@ethereumjs/vm": "5.9.3",
    "@greymass/eosio": "0.6.11",
    "assert": "2.0.0",
    "bn.js": "5.2.1",
    "bull": "4.10.4",
    "collections": "5.1.13",
    "eosjs": "22.1.0",
    "ethereum-tx-decoder": "3.0.0",
    "ethereumjs-tx": "2.1.2",
    "ethereumjs-units": "0.2.0",
    "ethereumjs-util": "7.1.5",
    "ethers": "5.7.2",
    "keccak": "3.0.3",
    "lru-cache": "7.18.3",
    "moment": "2.29.4",
    "node-fetch": "2.6.9",
    "rlp": "3.0.0",
    "segfault-handler": "^1.3.0",
    "sha1-uint8array": "0.10.5",
    "text-encoding": "0.7.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.15.0",
    "winston": "3.8.2",
    "workerpool": "^8.0.0",
    "ws": "7.5.9"
  },
  "devDependencies": {
    "@ethereumjs/blockchain": "5.5.3",
    "@types/bn.js": "5.1.1",
    "@types/chai": "^4.3.11",
    "@types/debug": "4.1.7",
    "@types/elliptic": "^6.4.14",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lru-cache": "7.10.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/readable-stream": "2.3.15",
    "@types/text-encoding": "0.0.36",
    "@types/ws": "8.5.4",
    "chai": "^4.3.10",
    "eosjs-ecc": "4.0.7",
    "leap-mock": "0.1.1-rc6",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^10.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
