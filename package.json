    "@wharfkit/antelope": "^1.0.10",
    "bufferutil": "^4.0.8",
    "commander": "^11.1.0",
    "ethereum-cryptography": "2.1.2",
    "moment": "2.29.4",
    "node-fetch": "2.6.9",
